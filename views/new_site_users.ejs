<%- include(`partials/header`); -%>
<form action="/newsite" method="post">
    <label for="division-id">DIVISION ID :</label>
    <input type="text" id="division-id" name="division-id" required>

    <label for="email">EMAIL :</label>
    <input type="text" id="email" name="email" required>

    <label for="password">PASSWORD :</label>
    <input type="text" id="password" name="password" required>

    <label for="site-id">SITE ID :</label>
    <input type="text" id="site-id" name="site-id" required>

    <label for="country">COUNTRY :</label>
    <input type="text" id="country" name="country" required>

    <label for="state">STATE :</label>
    <input type="text" id="state" name="state" required>

    <label for="district">DISTRICT :</label>
    <input type="text" id="district" name="district" required>

    <label for="taluk">TALUK :</label>
    <input type="text" id="taluk" name="taluk" required>

    <label for="site-name">SITE NAME :</label>
    <input type="text" id="site-name" name="site-name" required>

    <label for="village">VILLAGE OR CITY :</label>
    <input type="text" id="village" name="village" required>

    <label for="pid">PID :</label>
    <input type="text" id="pid" name="pid" required>

    <label for="khatha-no">KHATHA OR PROPERTY NUMBER :</label>
    <input type="text" id="khatha-no" name="khatha-no" required>

    <label for="khathadar-name">NAME OF KHATHADAR :</label>
    <input type="text" id="khathadar-name" name="khathadar-name" required>

    <label for="type-of-building">TYPE OF BUILDING :</label>
    <input type="text" id="type-of-building" name="type-of-building" required>

    <button type="submit">Submit</button>
</form>
<%- include(`partials/footer`); -%>